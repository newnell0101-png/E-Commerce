# CameroonMart Environment Variables

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_project_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Optional: For production deployments
# VITE_SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# Application Configuration
VITE_APP_NAME=CameroonMart
VITE_APP_VERSION=2.0.0
VITE_APP_ENVIRONMENT=development

# Feature Flags
VITE_ENABLE_3D_VIEWER=true
VITE_ENABLE_VOICE_SEARCH=true
VITE_ENABLE_CHAT_SYSTEM=true
VITE_ENABLE_ANALYTICS=false

# API Configuration
VITE_API_BASE_URL=http://localhost:3000/api
VITE_WEBSOCKET_URL=ws://localhost:3000

# Payment Configuration (Demo Mode)
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_demo_key
VITE_PAYMENT_MODE=demo

# Upload Configuration
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/webp,image/gif

# Security Configuration
VITE_ENABLE_CSP=true
VITE_ENABLE_HTTPS_REDIRECT=false